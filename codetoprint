class OnlineShop {
private:
    string productName;
    int totalItems;

public:
    OnlineShop() {
        totalItems = 0;
    }

    void initialize() {
        totalItems = 0;
    }

    void calculateTotalItems(int items) {
        totalItems += items;
    }

    int getTotalItems() {
        return totalItems;
    }
};