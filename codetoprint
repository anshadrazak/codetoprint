void push(struct Stack* stack, int ele) {
    Node* newNode = new Node;
    newNode->data = ele;
    newNode->next = stack->top;
    stack->top = newNode;
}

void printstack(struct Stack* stack) {
    Node* temp = stack->top;
    while (temp != NULL) {
        cout << temp->data << " ";
        temp = temp->next;
    }
}

void deleteeven(struct Stack* stack) {
    Stack tempStack;
    tempStack.top = NULL;

    Node* curr = stack->top;

    // Keep only odd numbers
    while (curr != NULL) {
        if (curr->data % 2 != 0) {
            push(&tempStack, curr->data);
        }
        curr = curr->next;
    }

    // Reverse stack back to original order
    stack->top = NULL;
    curr = tempStack.top;
    while (curr != NULL) {
        push(stack, curr->data);
        curr = curr->next;
    }

    printstack(stack);
}