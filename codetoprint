#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<string> names(n);

    for (int i = 0; i < n; i++) {
        cin >> names[i];
    }

    // Print initial order
    cout << "Initial order: ";
    for (int i = 0; i < n; i++) {
        cout << names[i];
        if (i != n - 1)
            cout << " ";
    }
    cout << endl;

    // Insertion Sort
    for (int i = 1; i < n; i++) {
        string key = names[i];
        int j = i - 1;

        while (j >= 0 && names[j] > key) {
            names[j + 1] = names[j];
            j--;
        }
        names[j + 1] = key;

        // Print after each iteration
        cout << "After Iteration " << i << ": ";
        for (int k = 0; k < n; k++) {
            cout << names[k];
            if (k != n - 1)
                cout << " ";
        }
        cout << endl;
    }

    // Print sorted order
    cout << "Sorted order: ";
    for (int i = 0; i < n; i++) {
        cout << names[i];
        if (i != n - 1)
            cout << " ";
    }

    return 0;
}