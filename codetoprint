
#include <iostream>
using namespace std;

// Node structure for a Doubly Linked List
struct Node {
    int data;
    Node* next;
    Node* prev;
};

int main() {
    int n;
    cin >> n;

    Node* head = NULL;
    Node* tail = NULL;

    // Insertion loop (Appends to the tail, building the list: 47 -> 39 -> 24 -> 16)
    for (int i = 0; i < n; i++) {
        int x;
        cin >> x;
        Node* newNode = new Node;
        newNode->data = x;
        newNode->next = NULL;
        newNode->prev = tail; 
        
        if (tail) {
            tail->next = newNode;
        } else {
            head = newNode;
        }
        tail = newNode;
    }

    int p;
    // Position to delete
    cin >> p;

    // --- Output Before Deletion (Prints: Head -> Tail) ---
    cout << "Before deletion: " << endl;
    Node* t = head;
    while (t) {
        cout << t->data << " ";
        t = t->next;
    }
    cout << endl;

    // --- DELETION LOGIC ---
    
    // Safety check: Invalid position or empty list
    if (p <= 0 || head == nullptr) {
        // No action needed.
    }
    
    // Case 1: Deleting the Head (p = 1)
    else if (p == 1) {
        Node* d = head;
        head = head->next;
        if (head) {
            head->prev = NULL; 
        } else {
            tail = NULL; // List is now empty
        }
        delete d;
    } 
    
    // Case 2: Deleting a Middle or Tail Node (p > 1)
    else {
        Node* cur = head;
        
        // Traverse 'cur' to the node at position 'p'
        for (int i = 1; i < p && cur != nullptr; i++) {
            cur = cur->next;
        }

        // Check if the requested position 'p' was valid
        if (cur == nullptr) {
            // Position p is out of bounds. No action needed.
            
        } else {
            Node* d = cur; // The node to delete

            // 1. Update the 'next' pointer of the PREVIOUS node
            if (d->prev != nullptr) {
                d->prev->next = d->next;
            }

            // 2. Update the 'prev' pointer of the NEXT node
            if (d->next != nullptr) {
                d->next->prev = d->prev;
            }

            // 3. Update the 'tail' pointer if the tail was deleted
            if (d == tail) {
                tail = d->prev;
            }

            // 4. Free memory
            delete d;
        }
    } 
    
    // --- Output After Deletion (Prints: Head -> Tail) ---
    cout << "After deletion: " << endl;
    t = head;
    while (t) {
        cout << t->data << " ";
        t = t->next;
    }
    cout << endl;

    // Clean up remaining list memory
    Node* current = head;
    while (current) {
        Node* next = current->next;
        delete current;
        current = next;
    }

    return 0;
}
