Message* reverseInsertedMessages(Message* head) {
    if (head == nullptr || head->next == head)
        return head;

    Message* prev = nullptr;
    Message* curr = head;
    Message* next = nullptr;

    // Because circular, loop until we return to head
    do {
        next = curr->next;
        curr->next = prev;
        prev = curr;
        curr = next;
    } while (curr != head);

    // Now 'head' should point to last processed node
    head->next = prev;

    // new head is 'prev'
    return prev;
}