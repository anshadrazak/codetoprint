#include <iostream>
using namespace std;

class OnlineShop {
    string productName;
    int a, b, c;

public:
    void readData() {
        cin >> productName;
        cin >> a >> b >> c;
    }

    int totalItems() {
        return a + b + c;
    }

    void display() {
        cout << "Product Name: " << productName << endl;
        cout << "Total items: " << totalItems();
    }
};

int main() {
    OnlineShop obj;
    obj.readData();
    obj.display();
    return 0;
}