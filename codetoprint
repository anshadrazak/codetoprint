#include <iostream>
using namespace std;

struct SolarPanel {
    int panelID;
    double hoursOfSunlight;
    double efficiencyRating;
    double energyProduced;
    string energyClassification;
};

int main() {
    int n;
    cin >> n;
    SolarPanel panel[n];

    for(int i = 0; i < n; i++) {
        cin >> panel[i].panelID >> panel[i].hoursOfSunlight >> panel[i].efficiencyRating;

        panel[i].energyProduced = panel[i].hoursOfSunlight * panel[i].efficiencyRating;

        if(panel[i].energyProduced > 8.0)
            panel[i].energyClassification = "Excellent";
        else if(panel[i].energyProduced > 4.0)
            panel[i].energyClassification = "Not Bad";
        else if(panel[i].energyProduced > 2.0)
            panel[i].energyClassification = "Average";
        else
            panel[i].energyClassification = "Poor";
    }

    for(int i = 0; i < n; i++) {
        cout << panel[i].panelID << " " << panel[i].energyProduced << " " << panel[i].energyClassification << endl;
    }
    return 0;
}