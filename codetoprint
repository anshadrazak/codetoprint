#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    int n, m;
    cin >> n;
    vector<int> arr1(n);
    for(int i = 0; i < n; i++)
        cin >> arr1[i];

    cin >> m;
    vector<int> arr2(m);
    for(int i = 0; i < m; i++)
        cin >> arr2[i];

    // Merge the arrays
    vector<int> merged;
    merged.reserve(n + m);
    merged.insert(merged.end(), arr1.begin(), arr1.end());
    merged.insert(merged.end(), arr2.begin(), arr2.end());

    // Sort the merged array
    sort(merged.begin(), merged.end());

    // Print merged sorted array
    for(int i = 0; i < merged.size(); i++) {
        cout << merged[i];
        if(i != merged.size() - 1) cout << " ";
    }

    return 0;
}
