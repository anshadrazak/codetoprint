#include <iostream>
#include <iomanip>
#include <vector>
#include <string>
using namespace std;

struct SolarPanel {
    int panelID;
    double hoursOfSunlight;
    double efficiencyRating;
    double energyProduced;
    string energyClassification;
};

string classify(double energyProduced) {
    if (energyProduced >= 8.0)
        return "Excellent";
    else if (energyProduced >= 4.0)
        return "Not Bad";
    else if (energyProduced >= 2.0)
        return "Average";
    else
        return "Poor";
}

int main() {
    int n;
    cin >> n;
    vector<SolarPanel> panels(n);
    for (int i = 0; i < n; i++) {
        cin >> panels[i].panelID >> panels[i].hoursOfSunlight >> panels[i].efficiencyRating;
        panels[i].energyProduced = (panels[i].hoursOfSunlight * panels[i].efficiencyRating) / 100.0;
        panels[i].energyClassification = classify(panels[i].energyProduced);
    }
    for (int i = 0; i < n; i++) {
        cout << panels[i].panelID << " "
             << fixed << setprecision(2) << panels[i].energyProduced << " "
             << panels[i].energyClassification << endl;
    }
    return 0;
}