#include <iostream>
#include <iomanip>
#include <vector>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<float> stocks(n);
    
    for (int i = 0; i < n; i++)
        cin >> stocks[i];
    
    // Print initial order
    cout << fixed << setprecision(2);
    cout << "Initial order: ";
    for (int i = 0; i < n; i++) {
        cout << stocks[i];
        if (i != n - 1) cout << " ";
    }
    cout << endl;

    // Insertion sort with intermediate output
    for (int i = 1; i < n; i++) {
        float key = stocks[i];
        int j = i - 1;

        while (j >= 0 && stocks[j] > key) {
            stocks[j + 1] = stocks[j];
            j--;
        }
        stocks[j + 1] = key;

        cout << "After Iteration " << i << ": ";
        for (int k = 0; k < n; k++) {
            cout << stocks[k];
            if (k != n - 1) cout << " ";
        }
        cout << endl;
    }

    // Print final sorted order
    cout << "Sorted order: ";
    for (int i = 0; i < n; i++) {
        cout << stocks[i];
        if (i != n - 1) cout << " ";
    }

    return 0;
}