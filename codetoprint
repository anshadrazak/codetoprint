#include <iostream>
#include <map>
using namespace std;

int main() {
    int n;
    cin >> n;
    map<int, int> freq;  // Keeps keys sorted automatically

    for (int i = 0; i < n; i++) {
        int num;
        cin >> num;
        freq[num]++;
    }

    for (auto &p : freq) {
        cout << p.first << ": " << p.second << endl;
    }

    return 0;
}
