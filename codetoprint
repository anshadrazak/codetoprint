#include <iostream>
#define max 5
using namespace std;

// Insert function
int insertq(int queue[max], int& rear, int data) {
    if (rear == max - 1) {
        return 0; // queue full
    } else {
        rear++;
        queue[rear] = data;
        return 1; // success
    }
}

// Delete function
int delq(int queue[max], int& front, int& rear, int& data) {
    if (front == rear) {
        return 0; // queue empty
    } else {
        front++;
        data = queue[front];

        // Reset when queue becomes empty
        if (front == rear) {
            front = rear = -1;
        }

        return 1; // success
    }
}

// Display function
void display(int queue[max], int front, int rear) {
    if (front == rear) {
        cout << "Queue is empty" << endl;
    } else {
        cout << "Elements in the queue are: ";
        for (int i = front + 1; i <= rear; i++) {
            cout << queue[i] << " ";
        }
        cout << endl;
    }
}

int main() {
    int queue[max], data;
    int front, rear;
    char reply;
    int option;

    front = rear = -1;

    while (true) {
        if (!(cin >> option))
            break;

        switch (option) {
        case 1:
            if (!(cin >> data))
                break;

            reply = insertq(queue, rear, data);
            if (reply == 0) {
                cout << "Queue is full" << endl;
            } else {
                cout << data << " is inserted in the queue" << endl;
            }
            break;

        case 2:
            reply = delq(queue, front, rear, data);
            if (reply == 0) {
                cout << "Queue is empty" << endl;
            } else {
                cout << "Deleted number is: " << data << endl;
            }
            break;

        case 3:
            display(queue, front, rear);
            break;

        case 4:
            cout << "Exiting the program" << endl;
            return 0;

        default:
            cout << "Invalid option" << endl;
            break;
        }
    }
    return 0;
}