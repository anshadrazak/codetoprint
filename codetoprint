#include <iostream>
#include <vector>
#include <cmath>
#include <algorithm>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<int> books(n);
    for (int i = 0; i < n; i++)
        cin >> books[i];

    int x, k;
    cin >> x >> k;

    // Binary search to find insert position for x
    int left = 0, right = n - 1;
    while (left <= right) {
        int mid = left + (right - left) / 2;
        if (books[mid] < x)
            left = mid + 1;
        else
            right = mid - 1;
    }

    // After loop: 'right' points to smaller, 'left' points to larger or equal
    int l = right;
    int r = left;

    vector<int> result;

    // Pick k closest elements
    while (k-- > 0) {
        if (l >= 0 && r < n) {
            if (abs(books[l] - x) <= abs(books[r] - x))
                result.push_back(books[l--]);
            else
                result.push_back(books[r++]);
        } else if (l >= 0)
            result.push_back(books[l--]);
        else if (r < n)
            result.push_back(books[r++]);
    }

    sort(result.begin(), result.end());

    for (int i = 0; i < result.size(); i++) {
        cout << result[i];
        if (i != result.size() - 1) cout << " ";
    }

    return 0;
}