#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<int> arr(n);

    for (int i = 0; i < n; i++)
        cin >> arr[i];

    // Print initial array
    cout << "Initial array: ";
    for (int i = 0; i < n; i++)
        cout << arr[i] << (i == n - 1 ? '\n' : ' ');

    // Selection sort with iteration display
    for (int i = 0; i < n; i++) {
        int minIndex = i;
        for (int j = i + 1; j < n; j++) {
            if (arr[j] < arr[minIndex])
                minIndex = j;
        }
        swap(arr[i], arr[minIndex]);

        cout << "After iteration " << i + 1 << ": ";
        for (int k = 0; k < n; k++)
            cout << arr[k] << (k == n - 1 ? '\n' : ' ');
    }

    // Final sorted array
    cout << "Final sorted array: ";
    for (int i = 0; i < n; i++)
        cout << arr[i] << (i == n - 1 ? '\n' : ' ');

    return 0;
}
