void findMostRepeated(Deque dq) {
    if (dq.front == NULL) return;

    int freq[101] = {0};

    Node* temp = dq.front;
    int firstElement = temp->value;

    while (temp != NULL) {
        freq[temp->value]++;
        temp = temp->next;
    }

    int maxCount = 1;
    int mostRepeated = firstElement;

    for (int i = 1; i <= 100; i++) {
        if (freq[i] > maxCount) {
            maxCount = freq[i];
            mostRepeated = i;
        }
    }

    if (maxCount == 1) {
        cout << "Most Repeated Element is " << firstElement;
    } else {
        cout << "Most Repeated Element is " << mostRepeated 
             << " with Count " << maxCount;
    }
}