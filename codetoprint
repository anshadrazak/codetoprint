#include <iostream>
using namespace std;

struct Node {
    string data;
    Node* prev;
    Node* next;
};

void insertEnd(Node* &head, Node* &tail, string value) {
    Node* newNode = new Node;
    newNode->data = value;
    newNode->next = NULL;

    if(head == NULL) {
        newNode->prev = NULL;
        head = tail = newNode;
    } else {
        newNode->prev = tail;
        tail->next = newNode;
        tail = newNode;
    }
}

void display(Node* head) {
    Node* temp = head;
    while(temp != NULL) {
        cout << temp->data;
        temp = temp->next;
        if(temp != NULL) cout << " ";
    }
    cout << endl;
}

int main() {
    int n;
    cin >> n;

    Node *head = NULL, *tail = NULL;

    for(int i = 0; i < n; i++) {
        string name;
        cin >> name;
        insertEnd(head, tail, name);
    }

    int mid = (n + 1) / 2; // first half gets extra if odd
    Node *temp = head;
    
    for(int i = 1; i < mid; i++) {
        temp = temp->next;
    }

    Node *secondHead = temp->next;
    if(secondHead != NULL) secondHead->prev = NULL;
    temp->next = NULL;

    display(head);
    display(secondHead);

    return 0;
}