#include <iostream>
using namespace std;

struct Node {
    char data;
    Node* prev;
    Node* next;
};

void insertEnd(Node* &head, Node* &tail, char value) {
    Node* newNode = new Node;
    newNode->data = value;
    newNode->next = NULL;

    if(head == NULL) {
        newNode->prev = NULL;
        head = tail = newNode;
    } else {
        newNode->prev = tail;
        tail->next = newNode;
        tail = newNode;
    }
}

int length(Node* head) {
    int count = 0;
    while(head != NULL) {
        count++;
        head = head->next;
    }
    return count;
}

void rotateLeft(Node* &head, Node* &tail, int k) {
    if(head == NULL || k == 0) return;

    int len = length(head);
    k = k % len;  
    if(k == 0) return;

    Node* temp = head;
    for(int i = 0; i < k - 1; i++)
        temp = temp->next;

    Node* newHead = temp->next;
    newHead->prev = NULL;
    temp->next = NULL;

    tail->next = head;
    head->prev = tail;

    head = newHead;
    tail = temp;
}

void display(Node* head) {
    Node* temp = head;
    while(temp != NULL) {
        cout << temp->data << " ";
        temp = temp->next;
    }
}

int main() {
    int n;
    cin >> n;

    Node* head = NULL;
    Node* tail = NULL;

    for(int i = 0; i < n; i++) {
        char ch;
        cin >> ch;
        insertEnd(head, tail, ch);
    }

    int k;
    cin >> k;

    rotateLeft(head, tail, k);
    display(head);

    return 0;
}