#include <iostream>
using namespace std;

void printHistogram(int *arr, int n) {
    // Check for input validity
    if (n < 1 || n > 20) {
        cout << "Invalid" << endl;
        return;
    }
    for (int i = 0; i < n; i++) {
        if (*(arr + i) < 1 || *(arr + i) > 10) {
            cout << "Invalid" << endl;
            return;
        }
    }

    // Print histogram
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < *(arr + i); j++) {
            cout << "x";
        }
        cout << endl;
    }
}

int main() {
    int n;
    cin >> n;
    int heights[20];
    for (int i = 0; i < n; i++) {
        cin >> heights[i];
    }
    printHistogram(heights, n);
    return 0;
}