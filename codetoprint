#include <iostream>
using namespace std;

struct link {
    int info;
    link* next;
};

struct link* create_header_list(int data) {
    link* newNode = new link();
    newNode->info = data;
    newNode->next = NULL;
    return newNode;
}

struct link* insertData(link* start, int data) {
    link* newNode = new link();
    newNode->info = data;
    newNode->next = NULL;

    if (start == NULL) {
        start = newNode;
    } else {
        link* temp = start;
        while (temp->next != NULL)
            temp = temp->next;
        temp->next = newNode;
    }
    return start;
}

struct link* display(link* start) {
    link* temp = start;
    while (temp != NULL) {
        cout << temp->info << " ";
        temp = temp->next;
    }
    return start;
}