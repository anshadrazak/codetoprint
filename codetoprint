#include <iostream>
#include <vector>
using namespace std;

int main() {
    int N;
    cin >> N;
    vector<int> heights(N);
    for (int i = 0; i < N; ++i) {
        cin >> heights[i];
    }

    // Print before sorting
    cout << "Student's height order before sorting: ";
    for (int i = 0; i < N; ++i) {
        cout << heights[i];
        if (i != N - 1) cout << " ";
    }
    cout << endl;

    // Selection sort with intermediate outputs
    for (int i = 0; i < N - 1; ++i) {
        int min_idx = i;
        for (int j = i + 1; j < N; ++j) {
            if (heights[j] < heights[min_idx]) {
                min_idx = j;
            }
        }
        // Swap if needed
        if (min_idx != i) {
            int temp = heights[i];
            heights[i] = heights[min_idx];
            heights[min_idx] = temp;
        }

        cout << "Height order of students after iteration " << (i + 1) << ": ";
        for (int k = 0; k < N; ++k) {
            cout << heights[k];
            if (k != N - 1) cout << " ";
        }
        cout << endl;
    }

    // Final sorted order
    cout << "After final comparison of all students, the height order becomes: ";
    for (int i = 0; i < N; ++i) {
        cout << heights[i];
        if (i != N - 1) cout << " ";
    }
    cout << endl;

    return 0;
}
