#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
    Node* prev;
};

int main() {
    int n;
    cin >> n;

    Node* head = NULL;
    Node* tail = NULL;

    for (int i = 0; i < n; i++) {
        int x;
        cin >> x;
        Node* newNode = new Node;
        newNode->data = x;
        newNode->next = NULL;
        newNode->prev = tail;
        if (tail) tail->next = newNode;
        else head = newNode;
        tail = newNode;
    }

    int p;
    cin >> p;

    cout << "Before deletion: ";
    Node* t = tail;
    while (t) {
        cout << t->data << " ";
        t = t->prev;
    }
    cout << endl;

    if (p == 1) {
        Node* d = head;
        head = head->next;
        if (head) head->prev = NULL;
        else tail = NULL;
        delete d;
    } else {
        Node* cur = head;
        for (int i = 1; i < p; i++) cur = cur->next;
        Node* d = cur;
        if (d->prev) d->prev->next = d->next;
        if (d->next) d->next->prev = d->prev;
        if (d == tail) tail = d->prev;
        delete d;
    }

    cout << "After deletion: ";
    t = tail;
    while (t) {
        cout << t->data << " ";
        t = t->prev;
    }
}