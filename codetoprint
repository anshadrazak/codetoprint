#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    int n;
    cin >> n;
    float arr[n];

    for (int i = 0; i < n; i++)
        cin >> arr[i];

    cout << fixed << setprecision(2);

    // Bubble Sort
    for (int i = 0; i < n - 1; i++) {
        bool swapped = false;

        for (int j = 0; j < n - i - 1; j++) {
            if (arr[j] > arr[j + 1]) {
                swap(arr[j], arr[j + 1]);
                swapped = true;
            }
        }

        if (swapped) {
            cout << "After iteration " << i + 1 << ": ";
            for (int k = 0; k < n; k++) {
                cout << arr[k];
                if (k != n - 1) cout << " ";
            }
            cout << endl;
        } else {
            break; // No swap means array is sorted
        }
    }

    cout << "Sorted array: ";
    for (int i = 0; i < n; i++) {
        cout << arr[i];
        if (i != n - 1) cout << " ";
    }

    return 0;
}
