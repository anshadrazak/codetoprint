#include <iostream>
#include <fstream>
using namespace std;

int main() {
    int numberCount;
    cin >> numberCount;

    // Step 1: Check if file is empty
    ifstream checkFile("numbers.txt");

    bool isEmpty = (checkFile.peek() == ifstream::traits_type::eof());
    checkFile.close();

    if (isEmpty) {
        cout << "File is empty" << endl;
        cout << "Adding " << numberCount << " numbers starting from 100" << endl;

        ofstream updateFile("numbers.txt", ios::app);
        for (int i = 100; i < 100 + numberCount; i++) {
            updateFile << i << endl;
        }
        updateFile.close();

        cout << "Success";
    }

    return 0;
}