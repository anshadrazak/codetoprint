#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<string> strArr(n);
    vector<int> intArr(n);

    for (int i = 0; i < n; i++) {
        cin >> strArr[i];
    }

    for (int i = 0; i < n; i++) {
        cin >> intArr[i];
    }

    cout << "Merged Array: ";
    for (int i = 0; i < n; i++) {
        cout << strArr[i] << " " << intArr[i];
        if (i != n - 1) cout << " ";
    }

    return 0;
}
