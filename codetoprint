#include <iostream>
#include <iomanip>
using namespace std;

// Overloaded function for integer addition
void add(int a, int b, int c) {
    int sum = 0;
    sum += a;
    cout << sum << endl;
    sum += b;
    cout << sum << endl;
    sum += c;
    cout << sum << endl;
}

// Overloaded function for double addition
void add(double a, double b, double c) {
    double sum = 0;
    sum += a;
    cout << fixed << setprecision(1) << sum << endl;
    sum += b;
    cout << fixed << setprecision(1) << sum << endl;
    sum += c;
    cout << fixed << setprecision(1) << sum << endl;
}

int main() {
    int choice;
    cin >> choice;

    if (choice == 1) {
        int x, y, z;
        cin >> x >> y >> z;
        add(x, y, z);
    } 
    else if (choice == 2) {
        double x, y, z;
        cin >> x >> y >> z;
        add(x, y, z);
    }

    return 0;
}