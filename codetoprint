#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    int n;
    cin >> n;
    float arr[n];
    
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    cout << fixed << setprecision(2);
    cout << "Initial order: ";
    for (int i = 0; i < n; i++) {
        cout << arr[i];
        if (i != n - 1) cout << " ";
    }
    cout << endl;

    for (int i = 1; i < n; i++) {
        float key = arr[i];
        int j = i - 1;
        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j];
            j--;
        }
        arr[j + 1] = key;

        cout << "After iteration " << i << ": ";
        for (int k = 0; k < n; k++) {
            cout << arr[k];
            if (k != n - 1) cout << " ";
        }
        cout << endl;
    }

    cout << "Sorted order: ";
    for (int i = 0; i < n; i++) {
        cout << arr[i];
        if (i != n - 1) cout << " ";
    }

    return 0;
}
